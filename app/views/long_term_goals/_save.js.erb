<% if params[:date] || (request.referer == root_url) %>
  $(".calendar").html("<%= escape_javascript(render 'landing_page/calendar') %>")
  $("#goal-overview-wrapper").html("<%= escape_javascript(render 'landing_page/goal_overview') %>")
<% else %>
  $(".l-index").html("<%= escape_javascript(render 'l_goal') %>")  
<% end %>
$(".goal-modal").modal("hide")